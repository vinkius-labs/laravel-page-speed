{
    "name": "vinkius-labs/laravel-page-speed",
    "description": "Laravel Page Speed",
    "keywords": [
        "laravel",
        "page speed",
        "minify",
        "optimize",
        "html"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Renato Marinho",
            "email": "renato.marinho@gitscrum.com"
        }
    ],
    "require": {
        "php": "^8.2 || ^8.3",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0 || ^13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5 || ^11.0 || ^12.5.12",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "squizlabs/php_codesniffer": "^3.6",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "VinkiusLabs\\LaravelPageSpeed\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VinkiusLabs\\LaravelPageSpeed\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "VinkiusLabs\\LaravelPageSpeed\\ServiceProvider"
            ]
        }
    }
}
